{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "React Redux starter with all the latest babel features",
  "main": "index.js",
  "scripts": {
    "build": "run-s _coverage:clean _test:coverage _build:clean _build",
    "gen": "node ./generators-redux/run",
    "flow": "flow",
    "prebuild": "node scripts/prebuild.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --config=config/jest/conf.json",
    "test:snapshot": "jest --config=config/jest/conf.json --updateSnapshot",
    "_build": "node scripts/build.js",
    "_build:clean": "rimraf build",
    "_build:open": "npm-run-all build -p ___build:serve ___build:browser",
    "_coverage": "run-s _coverage:clean ___coverage:build",
    "_coverage:clean": "rimraf coverage",
    "_coverage:open": "npm-run-all _coverage -p ___coverage:serve ___coverage:browser",
    "_test:coverage": "run-s _coverage",
    "_test:nocoverage": "cross-env CI=true node scripts/test.js --config=config/jest/conf.json",
    "___build:browser": "opn http://localhost:7001",
    "___build:serve": "pushstate-server build 7001",
    "___coverage:build": "cross-env CI=true node scripts/test.js --config=config/jest/conf.coverage.json",
    "___coverage:browser": "opn http://localhost:7000",
    "___coverage:serve": "pushstate-server coverage/lcov-report 7000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alovelia/redux-simple-starter.git"
  },
  "keywords": [
    "redux",
    "react",
    "babel",
    "esnext"
  ],
  "author": "Roman Yudin | Margarita Yudina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alovelia/redux-simple-starter/issues"
  },
  "homepage": "https://github.com/Alovelia/redux-simple-starter#readme",
